Test-Scenario-13:
  Id: ts_13
  Path: hydratk/extensions/yoda/testengine/03_methods_ut.jedi
  Name: ts_codestack_execute
  Desc: Test CodeStack method execute
  Author: Petr Rasek <bowman@hydratk.org>
  Version: 0.1
  
  Pre-Req: |
    from hydratk.extensions.yoda.testengine import CodeStack  
    import sys
    
    try:
        from StringIO import StringIO
    except ImportError:
        from io import StringIO     
  
  Test-Case-1:
    Id: tc_01
    Name: tc_execute
    Desc: Execute code
    
    Test-Condition-1: 
      Id: tco_01
      Name: tco_locals
      Desc: Variable in locals
      
      Test: |
        old = sys.stdout
        sys.stdout = res = StringIO()      
        c = CodeStack()  
        val = '1'
        c.execute('print(xxx)', {'xxx': val})
        res = res.getvalue()
        sys.stdout = old        
        
      Validate: |
        this.test_result = res
        assert (res[0] == val), 'execute prints {0}'.format(val)    
        
    Test-Condition-2: 
      Id: tco_02
      Name: tco_globals
      Desc: Variable in globals
      
      Test: |   
        old = sys.stdout
        sys.stdout = res = StringIO()      
        val = '2' 
        globals()['xxx'] = val    
        c._locals = {} 
        c.execute('print xxx', {})
        res = res.getvalue()
        sys.stdout = old         
        
      Validate: |
        this.test_result = res
        assert (res[0] == val), 'execute prints {0}'.format(val)                                                             